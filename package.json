{
	"name": "launch-mocha-tests",
	"displayName": "Launch Mocha Tests",
	"description": "Launch Mocha tests",
	"version": "0.0.1",
	"publisher": "sandy081",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:launchMochaTests.debugTest"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "launchMochaTests.debugTest",
				"title": "Mocha: Debug test..."
			}
		],
		"configuration": {
			"title": "Launch Mocha Tests",
			"properties": {
				"launch.mocha.ts.outDir": {
					"type": "string",
					"default": "out",
					"description": "out directory (relative to workspace) for typescript files."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	}
}